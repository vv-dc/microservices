{
    "RabbitMqConnection": {
        "Host": "localhost",
        "Port": "5672",
        "User": "guest",
        "Password": "guest"
    },
    
    "UserChangedEventConsumerOptions": {
        "ExchangeName": "custom-exchange",
        "RoutingKey": "customer-event"
    },

    "Serilog": {
        "Using": [ "Serilog.Sinks.Console" ],
        "MinimumLevel": {
            "Default": "Information"
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
                }
            }
        ]
    }
}